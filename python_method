#字符串
.upper() #字符串中所有字母变大写
.lower() #字符串中所有字母变小写
.find('x') #在字符串中寻找x的位置
.replace('x','y') #用y代替x
.split(' ') #以' '(空格)为界 将字符串分割为一个列表中的多个键 'Hi boy'->['Hi', 'boy']

#列表
.append(5) #将5加入列表并置于最后一位
.pop() #删去列表中的最后一个键
.reverse() #将列表降序排列
.sort() #将列表升序排列
.insert(5,10) #将10插入第5个键的后方 成为第六个键
.index(5) #查询5在列表中的位置
.count(5) #数一数列表中有几个5
.copy() #复制列表
